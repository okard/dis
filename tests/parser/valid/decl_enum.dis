//# desc: Declaration - Enum
//# type: Runnable
//# compile-cmd: {disc} -parse {srcfile} -o {outname}
//# compile-result: 0
//# run-cmd: {exc}
//# run-result: 0

//simple
enum A
{
    S,
    SS,
    Foo
}

//with type
enum B : uint8
{
    ABC,
    BCD,
    Foo
}


//with value expression
enum C
{
    A = 1,
    B = 0xff,
    C = 0b101010
}


//type with values
enum D : uint8
{
    A = 1 << 0,
    B = 1 << 1,
    C = 1 << 2
}

enum E
{
	A(~i8),
	B(u8)
}

//complex enum
enum F
{
    A(name: i8),
    B(id: u8, fid: i8)
}
