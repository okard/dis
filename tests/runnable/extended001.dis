
//# Desc: interface + extension method
//# CompileResult: 0
//# RunResult: 0
//# Type: Runnable

package tests.runnable.extended001;

import std.io;

//class
obj foo
{
    var i: int = 0;
}

//interface
trait Printable
{
    def print() : void;
}

//extension method
def print(this f: foo): void = writeln(f.i.toString());

//main
def main
{
    var f: Printable = foo();
    f.print();
}


