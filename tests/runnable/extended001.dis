
//# Desc: interface + extension method
//# CompileResult: 0
//# RunResult: 0
//# Type: Runnable

package tests.runnable.extended001;

import std.io;

//interface
trait Printable
{
    def print() : void;
}

//class
obj foo
{
    var i: int32 = 4;
}
def print(this f: foo): void = writeln(f.i.toString());

// primary type
var i: int32 = 5;
def print(this i: int32): void = writeln(i.toString());

//main
def main
{
    var f: Printable = foo();
    f.print();
    i.print();
}


